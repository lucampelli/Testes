package Main;

public class Calculadora {
	
	int ultimoNumero = 0;

	public int somar(int i, int j) {
		ultimoNumero = i + j;
		return ultimoNumero;
	}

	public int subtrair(int i, int j) {
		ultimoNumero = i - j;
		return ultimoNumero;
	}
	
	public int somar(int i) {
		ultimoNumero = ultimoNumero + i;
		return ultimoNumero;
	}

	public int subtrair(int i) {
		ultimoNumero = ultimoNumero -i;
		return ultimoNumero;
	}

}
